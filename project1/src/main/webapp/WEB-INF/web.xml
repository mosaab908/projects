<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>employees</servlet-name>
        <servlet-class>company.controller.EmployeeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>employees</servlet-name>
        <url-pattern>/employees/*</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>tickets</servlet-name>
        <servlet-class>company.controller.TicketServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>tickets</servlet-name>
        <url-pattern>/tickets</url-pattern>
    </servlet-mapping>

</web-app>
